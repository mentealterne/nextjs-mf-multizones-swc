{
  "name": "@rad/libraries",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "resolutions": {
    "eslint-config-rad": "workspace:*"
  },
  "dependencies": {
    "eslint-config-rad": "1.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && yarn build:swc",
    "build:swc": "swc src -d dist",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo",
    "types": "tsc --emitDeclarationOnly"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": [
        "./dist/*.js",
        "./dist/*/index.js"
      ],
      "require": [
        "./dist/*.cjs",
        "./dist/*/index.cjs"
      ]
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ]
}
